<h1>My Events:</h1>
<% if user_signed_in? %> 
  <% @user.each do |event| %>
    <h3><%="- " + event.title %></h3>
    <p><%= event.date %></p>
    <p><%= event.location %></p>
    <p><%= event.description %></p>
  <% end %>
<% else %>
  <%= button_to "Sign in", new_user_session_path %> 
<% end %>


